{
    "name": "@neshca/cache-testing",
    "version": "0.0.0",
    "private": true,
    "license": "MIT",
    "scripts": {
        "build": "tsup && next build && ./create-instances.sh",
        "clean": "rimraf ./.next ./dist ./.turbo ./node_modules ./playwright-report",
        "cluster:start": "pm2 start cluster.config.js --env production",
        "cluster:stop": "pm2 kill",
        "dev": "tsup --watch",
        "dev:app": "next dev",
        "e2e": "playwright test --config=./playwright.config.ts",
        "e2e:ui": "playwright test --ui --config=./playwright.config.ts",
        "lint": "next lint",
        "start": "REDIS_URL=redis://localhost:6379 SERVER_STARTED=1 node .next/__instances/3000/server.js"
    },
    "dependencies": {
        "lru-cache": "10.0.1",
        "next": "14.0.1",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "undici": "5.27.0"
    },
    "devDependencies": {
        "@neshca/cache-handler": "*",
        "@neshca/json-replacer-reviver": "*",
        "@neshca/tsconfig": "*",
        "@next/eslint-plugin-next": "14.0.1",
        "@playwright/test": "1.39.0",
        "@types/node": "20.8.10",
        "@types/react": "18.2.34",
        "@types/react-dom": "18.2.14",
        "eslint-config-neshca": "*",
        "fastify": "4.24.3",
        "pm2": "5.3.0",
        "redis": "4.6.10",
        "rimraf": "5.0.5",
        "tsup": "7.2.0",
        "typescript": "5.2.2"
    }
}
